{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "module": "ESNext",
    "moduleResolution": "bundler",
    "target": "ESNext",
    "types": ["bun-types"],
    "outDir": "./dist",
    "noEmit": false,
    "paths": {
      "@/*": ["./src/*"]
